    "babel-core": "^6.13.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2016": "^6.11.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016"
    ],
    "plugins": [
